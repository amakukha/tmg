	_tn,
#define _pn	(tword)&labels[1]
	_pn,
	1 + _tn,
	1 + _pn,
// _tn:
	1,
// _pn:
	2,
};

// Global label addresses inside the driving table

const tword labels[] = {
	(tword)&start[4],
	(tword)&start[5],
};
